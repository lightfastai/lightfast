{
  "name": "@db/console",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "default": "./src/index.ts"
    },
    "./schema": "./src/schema/index.ts",
    "./constants/naming": "./src/constants/naming.ts",
    "./client": "./src/client.ts",
    "./utils": "./src/utils/workspace.ts",
    "./env": {
      "types": "./dist/env.d.ts",
      "default": "./env.ts"
    }
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "db:push": "pnpm with-env drizzle-kit push --config=./src/drizzle.config.ts",
    "db:generate": "pnpm with-env drizzle-kit generate --config=./src/drizzle.config.ts",
    "db:migrate": "pnpm with-env drizzle-kit migrate --config=./src/drizzle.config.ts",
    "db:studio": "pnpm with-env drizzle-kit studio --config=./src/drizzle.config.ts",
    "db:introspect": "pnpm with-env drizzle-kit introspect --config=./src/drizzle.config.ts",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../apps/console/.vercel/.env.development.local --"
  },
  "dependencies": {
    "@repo/console-types": "workspace:*",
    "@repo/lib": "workspace:*",
    "@t3-oss/env-nextjs": "catalog:",
    "@vendor/db": "workspace:*",
    "drizzle-orm": "catalog:",
    "drizzle-zod": "catalog:",
    "friendlier-words": "^1.1.3",
    "postgres": "^3.4.5",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@planetscale/database": "^1.19.0",
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "dotenv-cli": "^8.0.0",
    "drizzle-kit": "^0.31.4",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@repo/prettier-config"
}
