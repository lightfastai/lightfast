{
	"mcpServers": {
		"playwright-mastra": {
			"command": "npx",
			"args": [
				"@playwright/mcp@latest",
				"--headless",
				"--browser",
				"chrome",
				"--caps",
				"core,tabs,wait,files,testing",
				"--isolated",
				"--no-sandbox"
			]
		},
		"mastra-docs": {
			"command": "npx",
			"args": ["-y", "@mastra/mcp-docs-server"]
		},
		"container-use": {
			"command": "container-use",
			"args": ["stdio"],
			"timeout": 1200000
		}
	}
}