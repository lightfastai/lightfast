# OpenRouter Configuration
# Get your API key from https://openrouter.ai/keys
OPENROUTER_API_KEY=

# Clerk Authentication
# Get your keys from https://dashboard.clerk.com/
NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY=
CLERK_SECRET_KEY=

# Clerk URLs (Optional - defaults shown)
NEXT_PUBLIC_CLERK_SIGN_IN_URL=/sign-in
NEXT_PUBLIC_CLERK_SIGN_UP_URL=/sign-up
NEXT_PUBLIC_CLERK_AFTER_SIGN_IN_URL=/
NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL=/

# Inngest Configuration (Optional)
# Get your keys from https://app.inngest.com/
INNGEST_EVENT_KEY=
INNGEST_SIGNING_KEY=

# Node Environment
NODE_ENV=development

# AI Gateway API Key
# Get your key from https://vercel.com/dashboard/ai
AI_GATEWAY_API_KEY=

# PlanetScale Database
# Get your connection string from https://app.planetscale.com/
DATABASE_URL=mysql://username:password@host/database?ssl={"rejectUnauthorized":true}

# Vercel Sandbox Configuration
# VERCEL_OIDC_TOKEN is automatically set by Vercel CLI
# No manual configuration needed for sandbox creation

# Mastra Server Configuration (for MCP testing)
MASTRA_BASE_URL=http://localhost:4111
MASTRA_API_URL=http://localhost:4111/api

# Vercel Blob Storage
# Get your token from https://vercel.com/dashboard/stores
BLOB_READ_WRITE_TOKEN=

# Braintrust Configuration
# Get your API key from https://www.braintrust.dev/
# If not provided in development mode, will use local console logging
BRAINTRUST_API_KEY=
# Project ID for Braintrust evaluations (UUID format)
BRAINTRUST_PROJECT_ID=

# Upstash Redis Configuration (for resumable streams)
# Get your credentials from https://console.upstash.com/
UPSTASH_REDIS_URL=
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# Upstash KV Store Configuration
# Get your credentials from https://console.upstash.com/
UPSTASH_KV_URL=
UPSTASH_KV_REST_API_URL=
UPSTASH_KV_REST_API_TOKEN=
UPSTASH_KV_REST_API_READ_ONLY_TOKEN=

# AI Model API Keys
# Anthropic (Claude) - https://console.anthropic.com/
ANTHROPIC_API_KEY=

# OpenAI - https://platform.openai.com/
OPENAI_API_KEY=

# Exa Search - https://exa.ai/
EXA_API_KEY=

# BrowserBase - https://browserbase.com/
BROWSERBASE_API_KEY=
BROWSERBASE_PROJECT_ID=

# ElevenLabs (Optional) - https://elevenlabs.io/
ELEVENLABS_API_KEY=