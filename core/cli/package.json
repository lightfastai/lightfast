{
	"name": "@lightfastai/cli",
	"version": "0.2.1",
	"description": "CLI for Lightfast agent execution engine",
	"keywords": [
		"lightfast",
		"cli",
		"ai",
		"agents",
		"development"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/lightfastai/lightfast.git",
		"directory": "core/cli"
	},
	"homepage": "https://lightfast.ai",
	"bugs": "https://github.com/lightfastai/lightfast/issues",
	"license": "MIT",
	"author": "Lightfast <hello@lightfast.ai>",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"bin": {
		"cli": "./dist/index.js"
	},
	"files": [
		"dist",
		".output"
	],
	"scripts": {
		"cli:dev": "tsup --watch",
		"cli:build": "pnpm build:app && tsup",
		"prepublishOnly": "pnpm cli:build",
		"dev": "vite dev",
		"build:app": "vite build",
		"build": "vite build && tsc --noEmit",
		"start": "node .output/server/index.mjs",
		"typecheck": "tsc --noEmit",
		"lint": "eslint . --fix",
		"clean": "git clean -xdf .cache .turbo dist node_modules .output .vinxi"
	},
	"dependencies": {
		"@tanstack/react-router": "^1.131.28",
		"@tanstack/react-router-devtools": "^1.131.28",
		"@tanstack/react-start": "^1.131.28",
		"chalk": "^5.6.0",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
		"commander": "^12.1.0",
		"lucide-react": "^0.451.0",
		"react": "^19.1.1",
		"react-dom": "^19.1.1",
		"tailwind-merge": "^3.3.1",
		"zod": "^3.25.76"
	},
	"devDependencies": {
		"@repo/eslint-config": "workspace:*",
		"@repo/typescript-config": "workspace:*",
		"@tailwindcss/postcss": "^4.1.12",
		"@tailwindcss/typography": "^0.5.16",
		"@types/node": "^24.3.0",
		"@types/react": "^19.1.11",
		"@types/react-dom": "^19.1.8",
		"@vitejs/plugin-react": "^4.7.0",
		"postcss": "^8.5.6",
		"tailwindcss": "^4.1.12",
		"tsup": "^8.5.0",
		"tw-animate-css": "^1.3.7",
		"typescript": "^5.9.2",
		"vite": "^7.1.3",
		"vite-tsconfig-paths": "^5.1.4"
	},
	"engines": {
		"node": ">=18"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	}
}
