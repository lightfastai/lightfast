{
	"name": "@lightfastai/cli",
	"version": "0.3.1",
	"description": "CLI for Lightfast agent execution engine",
	"keywords": [
		"lightfast",
		"cli",
		"ai",
		"agents",
		"development"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/lightfastai/lightfast.git",
		"directory": "core/cli"
	},
	"homepage": "https://lightfast.ai",
	"bugs": "https://github.com/lightfastai/lightfast/issues",
	"license": "MIT",
	"author": "Lightfast <hello@lightfast.ai>",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"bin": {
		"cli": "./dist/index.js"
	},
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"build": "node scripts/build-all.js",
		"build:bundle": "tsup",
		"build:quick": "tsup",
		"dev": "tsup --watch",
		"typecheck": "tsc --noEmit",
		"lint": "eslint . --fix",
		"clean": "rm -rf dist .turbo node_modules .output .lightfast .nitro .tanstack"
	},
	"dependencies": {
		"chalk": "^5.6.0",
		"chokidar": "^4.0.3",
		"commander": "^12.1.0",
		"esbuild": "^0.25.9",
		"zod": "^3.25.76"
	},
	"peerDependencies": {
		"lightfast": ">=0.1.0"
	},
	"peerDependenciesMeta": {
		"lightfast": {
			"optional": true
		}
	},
	"devDependencies": {
		"@repo/eslint-config": "workspace:*",
		"@repo/typescript-config": "workspace:*",
		"@types/node": "^24.3.0",
		"tsup": "^8.5.0",
		"typescript": "^5.9.2"
	},
	"engines": {
		"node": ">=18"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org"
	}
}
