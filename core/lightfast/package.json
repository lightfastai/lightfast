{
	"name": "lightfast",
	"version": "0.2.1",
	"description": "AI agents, tools, and workflows for Lightfast",
	"keywords": [
		"ai",
		"agents",
		"llm",
		"typescript",
		"workflow"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/lightfastai/lightfast.git",
		"directory": "core/lightfast"
	},
	"homepage": "https://lightfast.ai",
	"bugs": "https://github.com/lightfastai/lightfast/issues",
	"license": "MIT",
	"author": "Lightfast <hello@lightfast.ai>",
	"main": "./dist/agent.mjs",
	"types": "./dist/agent.d.ts",
	"type": "module",
	"exports": {
		"./agent": {
			"types": "./dist/agent.d.ts",
			"import": "./dist/agent.mjs"
		},
		"./tool": {
			"types": "./dist/tool.d.ts",
			"import": "./dist/tool.mjs"
		},
		"./server/adapters/fetch": {
			"types": "./dist/server/adapters/fetch.d.ts",
			"import": "./dist/server/adapters/fetch.mjs"
		},
		"./server/adapters/types": {
			"types": "./dist/server/adapters/types.d.ts",
			"import": "./dist/server/adapters/types.mjs"
		},
		"./memory": {
			"types": "./dist/memory.d.ts",
			"import": "./dist/memory.mjs"
		},
		"./memory/adapters/in-memory": {
			"types": "./dist/memory/adapters/in-memory.d.ts",
			"import": "./dist/memory/adapters/in-memory.mjs"
		},
		"./memory/adapters/redis": {
			"types": "./dist/memory/adapters/redis.d.ts",
			"import": "./dist/memory/adapters/redis.mjs"
		},
		"./cache": {
			"types": "./dist/cache.d.ts",
			"import": "./dist/cache.mjs"
		},
		"./providers": {
			"types": "./dist/providers.d.ts",
			"import": "./dist/providers.mjs"
		},
		"./v2/agent": {
			"types": "./dist/v2/agent.d.ts",
			"import": "./dist/v2/agent.mjs"
		},
		"./v2/server": {
			"types": "./dist/v2/server.d.ts",
			"import": "./dist/v2/server.mjs"
		},
		"./v2/react": {
			"types": "./dist/v2/react.d.ts",
			"import": "./dist/v2/react.mjs"
		},
		"./v2/core": {
			"types": "./dist/v2/core.d.ts",
			"import": "./dist/v2/core.mjs"
		},
		"./v2": {
			"types": "./dist/v2/index.d.ts",
			"import": "./dist/v2/index.mjs"
		},
		"./v2/utils": {
			"types": "./dist/v2/utils.d.ts",
			"import": "./dist/v2/utils.mjs"
		},
		"./v2/events": {
			"types": "./dist/v2/events.d.ts",
			"import": "./dist/v2/events.mjs"
		},
		"./v2/env": {
			"types": "./dist/v2/env.d.ts",
			"import": "./dist/v2/env.mjs",
			"default": "./dist/v2/env.mjs"
		},
		"./v2/braintrust-env": {
			"types": "./dist/v2/braintrust-env.d.ts",
			"import": "./dist/v2/braintrust-env.mjs",
			"default": "./dist/v2/braintrust-env.mjs"
		}
	},
	"scripts": {
		"clean": "git clean -xdf .cache .turbo dist node_modules",
		"build": "tsup",
		"dev": "tsup --watch",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:coverage": "vitest run --coverage",
		"typecheck": "tsc --noEmit",
		"lint": "eslint . --fix"
	},
	"dependencies": {
		"@ai-sdk/anthropic": "catalog:",
		"@ai-sdk/gateway": "catalog:",
		"@ai-sdk/provider": "catalog:",
		"@t3-oss/env-core": "catalog:",
		"@upstash/qstash": "^2.8.1",
		"@upstash/redis": "catalog:",
		"ai": "catalog:",
		"braintrust": "catalog:",
		"pino": "^9.7.0",
		"resumable-stream": "catalog:",
		"uuid": "^11.1.0",
		"zod": "catalog:"
	},
	"devDependencies": {
		"@repo/eslint-config": "workspace:*",
		"@repo/typescript-config": "workspace:*",
		"@rollup/rollup-darwin-arm64": "^4.46.2",
		"@types/node": "^24.3.0",
		"@types/react": "^19.1.8",
		"@types/uuid": "^10.0.0",
		"@vitest/coverage-v8": "^3.2.4",
		"@vitest/ui": "^3.2.4",
		"rollup": "^4.46.2",
		"tsup": "^8.5.0",
		"typescript": "catalog:",
		"vite": "^7.1.2",
		"vitest": "^3.2.4"
	},
	"engines": {
		"node": ">=18"
	}
}
