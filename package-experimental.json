{
	"name": "vercel-sanbox-create",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"scripts": {
		"dev": "concurrently \"pnpm dev:next\" \"pnpm dev:mastra\"",
		"dev:next": "next dev",
		"dev:inngest": "npx inngest-cli@latest dev",
		"dev:mastra": "mastra dev --dir mastra",
		"build:mastra": "mastra build --dir mastra",
		"build": "next build",
		"start": "next start",
		"lint": "biome check --write .",
		"typecheck": "tsc --noEmit",
		"format": "biome format --write .",
		"test": "echo \"Error: no test specified\" && exit 1",
		"with-env": "dotenv -e .env.local --",
		"test:sandbox": "set -a && source .env.test 2>/dev/null && set +a && pnpm exec tsx lib/sandbox/sandbox-executor.test.ts",
		"test:sandbox:mock": "tsx lib/sandbox/sandbox-executor-mock.test.ts",
		"test:sandbox:integration": "./lib/sandbox/run-integration-tests.sh",
		"test:sandbox:scenarios": "set -a && source .env.test 2>/dev/null && set +a && pnpm exec tsx lib/sandbox/sandbox-executor-scenarios.test.ts",
		"test:sandbox:all": "pnpm test:sandbox && pnpm test:sandbox:integration && pnpm test:sandbox:scenarios",
		"playwright:install": "npx playwright install",
		"playwright:install-deps": "npx playwright install-deps",
		"test:e2e": "npx playwright test",
		"test:e2e:ui": "npx playwright test --ui",
		"test:e2e:headed": "npx playwright test --headed",
		"test:e2e:debug": "npx playwright test --debug"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"packageManager": "pnpm@10.5.2",
	"devDependencies": {
		"@biomejs/biome": "^2.1.1",
		"@executeautomation/playwright-mcp-server": "^1.0.6",
		"@playwright/test": "^1.54.0",
		"@tailwindcss/postcss": "^4.1.11",
		"@types/ms": "^2.1.0",
		"@types/node": "^24.0.12",
		"@types/react": "^19.1.8",
		"@types/react-dom": "^19.1.6",
		"autoprefixer": "^10.4.21",
		"concurrently": "^9.2.0",
		"dotenv-cli": "^8.0.0",
		"playwright": "^1.54.0",
		"postcss": "^8.5.6",
		"tailwindcss": "^4.1.11",
		"tsx": "^4.20.3",
		"tw-animate-css": "^1.3.5",
		"typescript": "^5.8.3"
	},
	"dependencies": {
		"@ai-sdk/anthropic": "^1.2.12",
		"@inngest/agent-kit": "^0.9.0",
		"@inngest/realtime": "^0.3.1",
		"@mastra/core": "^0.10.12",
		"@planetscale/database": "^1.19.0",
		"@radix-ui/react-label": "^2.1.7",
		"@radix-ui/react-scroll-area": "^1.2.9",
		"@radix-ui/react-select": "^2.2.5",
		"@radix-ui/react-slot": "^1.2.3",
		"@radix-ui/react-tabs": "^1.1.12",
		"@t3-oss/env-nextjs": "^0.13.8",
		"@vercel/sandbox": "^0.0.13",
		"ai": "^4.3.17",
		"class-variance-authority": "^0.7.1",
		"clsx": "^2.1.1",
