{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**", "dist/**"],
      "env": [
        "NODE_ENV",
        "NEXT_PUBLIC_*"
      ]
    },
    "dev": {
      "persistent": true,
      "cache": false
    },
    "typecheck": {
      "dependsOn": ["^typecheck"],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev:mastra": {
      "cache": false,
      "persistent": true
    },
    "build:mastra": {
      "dependsOn": ["^build"],
      "outputs": [".mastra/**"]
    },
    "clean": {
      "cache": false
    },
    "clean:all": {
      "cache": false
    },
    "start": {
      "dependsOn": ["build"],
      "persistent": true,
      "cache": false
    },
    "format": {
      "outputs": []
    },
    "test": {
      "outputs": [],
      "cache": false
    },
    "with-env": {
      "cache": false
    },
    "eval:a011": {
      "cache": false,
      "env": [
        "BRAINTRUST_*",
        "ANTHROPIC_API_KEY"
      ]
    },
    "eval:a011:dev": {
      "cache": false,
      "persistent": true,
      "env": [
        "BRAINTRUST_*",
        "ANTHROPIC_API_KEY",
        "SET_BASELINE"
      ]
    },
    "eval:a011:baseline": {
      "cache": false,
      "env": [
        "BRAINTRUST_*",
        "ANTHROPIC_API_KEY",
        "SET_BASELINE"
      ]
    },
    "eval:experiments": {
      "cache": false
    },
    "eval:list": {
      "cache": false
    },
    "eval:baseline": {
      "cache": false
    }
  }
}