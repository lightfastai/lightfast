{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json", "dist/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "format": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": [".cache/.eslintcache"]
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json"]
    },
    "clean": {
      "cache": false
    },
    "clean:all": {
      "cache": false
    },
    "start": {
      "dependsOn": ["build"],
      "persistent": true,
      "cache": false
    },
    "test": {
      "outputs": [],
      "cache": false
    },
    "with-env": {
      "cache": false
    },
    "dev:mastra": {
      "cache": false,
      "persistent": true
    },
    "build:mastra": {
      "dependsOn": ["^build"],
      "outputs": [".mastra/**"]
    },
    "eval:a011": {
      "cache": false,
      "interactive": true
    },
    "eval:a011:dev": {
      "cache": false,
      "persistent": true,
      "interactive": true
    },
    "eval:a011:baseline": {
      "cache": false,
      "interactive": true
    },
    "eval:experiments": {
      "cache": false,
      "interactive": true
    },
    "eval:list": {
      "cache": false,
      "interactive": true
    },
    "eval:baseline": {
      "cache": false,
      "interactive": true
    }
  },
  "globalEnv": [
    "REDIS_URL",
    "KV_URL",
    "KV_REST_API_URL",
    "KV_REST_API_TOKEN",
    "KV_REST_API_READ_ONLY_TOKEN",
    "ANTHROPIC_API_KEY",
    "OPENAI_API_KEY",
    "EXA_API_KEY",
    "BROWSERBASE_API_KEY",
    "BROWSERBASE_PROJECT_ID",
    "ELEVENLABS_API_KEY",
    "BRAINTRUST_API_KEY",
    "BRAINTRUST_PROJECT_ID",
    "BLOB_READ_WRITE_TOKEN",
    "AI_GATEWAY_API_KEY",
    "CLERK_SECRET_KEY",
    "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
    "NEXT_PUBLIC_VIRTUOSO_LICENSE_KEY",
    "NEXT_PUBLIC_VERCEL_ENV"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "npm_lifecycle_event",
    "ENVIRONMENT"
  ]
}