{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json", "dist/**"]
    },
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false,
      "persistent": false
    },
    "format": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": [".cache/.eslintcache"]
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json"]
    },
    "clean": {
      "cache": false
    },
    "lightfast:migrate": {
      "cache": false
    },
    "lightfast:migrate:generate": {
      "cache": false
    },
    "lightfast:studio": {
      "cache": false,
      "persistent": true
    },
    "media-server:migrate": {
      "cache": false
    },
    "media-server:migrate:generate": {
      "cache": false
    },
    "media-server:studio": {
      "cache": false,
      "persistent": true
    },
    "dev:email": {
      "cache": false,
      "persistent": true
    },
    "ui": {
      "cache": false,
      "interactive": true
    },
    "eval": {
      "cache": false,
      "interactive": true
    }
  },
  "globalEnv": [
    "PORT",
    "POSTGRES_URL",
    "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
    "CLERK_SECRET_KEY",
    "ARCJET_KEY",
    "RESEND_API_KEY",
    "NEXT_PUBLIC_API_URL",
    "INNGEST_APP_NAME",
    "INNGEST_EVENT_KEY",
    "INNGEST_SIGNING_KEY",
    "RESEND_EARLY_ACCESS_AUDIENCE_ID",
    "NEXT_PUBLIC_SENTRY_DSN",
    "SENTRY_ORG",
    "SENTRY_PROJECT",
    "SENTRY_AUTH_TOKEN",
    "NEXT_PUBLIC_SENTRY_ENVIRONMENT",
    "NEXT_PUBLIC_BETTER_STACK_SOURCE_TOKEN",
    "NEXT_PUBLIC_BETTER_STACK_INGESTING_URL",
    "NEXT_PUBLIC_LOGTAIL_SOURCE_TOKEN",
    "LOGTAIL_URL",
    "LOGTAIL_SOURCE_TOKEN"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "npm_lifecycle_event"
  ]
}
