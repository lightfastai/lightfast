{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".cache/tsbuildinfo.json",
        "dist/**",
        ".next/**",
        "!.next/cache/**",
        "!.next/dev/**"
      ],
      "env": [
        "NEXT_PUBLIC_*",
        "SENTRY_*",
        "VERCEL_ENV"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:inngest": {
      "cache": false,
      "persistent": true
    },
    "dev:qstash": {
      "cache": false,
      "persistent": true
    },
    "dev:ngrok": {
      "cache": false,
      "persistent": true
    },
    "dev:studio": {
      "cache": false,
      "persistent": true
    },
    "format": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "transit": {
      "dependsOn": ["^transit"]
    },
    "lint": {
      "dependsOn": ["transit"],
      "outputs": [".cache/.eslintcache"],
      "env": []
    },
    "typecheck": {
      "dependsOn": ["transit"],
      "outputs": [".cache/tsbuildinfo.json"],
      "env": []
    },
    "clean": {
      "cache": false
    },
    "migrate": {
      "cache": false
    },
    "migrate:generate": {
      "cache": false
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "dev:email": {
      "cache": false,
      "persistent": true
    },
    "ui": {
      "cache": false,
      "interactive": true
    },
    "eval": {
      "cache": false,
      "interactive": true
    }
  },
  "globalEnv": [
    "NODE_ENV",
    "CI"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "SKIP_ENV_VALIDATION",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "VERCEL_PROJECT_PRODUCTION_URL",
    "npm_lifecycle_event",
    "ENVIRONMENT",
    "DEBUG",
    "PORT",
    "POSTGRES_URL",
    "DATABASE_HOST",
    "DATABASE_USERNAME",
    "DATABASE_PASSWORD",
    "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
    "CLERK_SECRET_KEY",
    "NEXT_PUBLIC_CLERK_SIGN_IN_URL",
    "NEXT_PUBLIC_CLERK_SIGN_UP_URL",
    "NEXT_PUBLIC_CLERK_AFTER_SIGN_IN_URL",
    "NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL",
    "CLERK_MACHINE_SECRET_KEY_INNGEST",
    "CLERK_MACHINE_SECRET_KEY_WEBHOOK",
    "CLERK_MACHINE_SECRET_KEY_TRPC",
    "CLERK_M2M_TOKEN_WEBHOOK",
    "CLERK_M2M_MACHINE_ID_WEBHOOK",
    "CLERK_M2M_TOKEN_INNGEST",
    "CLERK_M2M_MACHINE_ID_INNGEST",
    "NEXT_PUBLIC_SENTRY_DSN",
    "SENTRY_DSN",
    "SENTRY_ORG",
    "SENTRY_PROJECT",
    "SENTRY_AUTH_TOKEN",
    "SENTRY_VERCEL_LOG_DRAIN_URL",
    "SENTRY_OTLP_TRACES_URL",
    "SENTRY_PUBLIC_KEY",
    "NEXT_PUBLIC_BETTER_STACK_SOURCE_TOKEN",
    "NEXT_PUBLIC_BETTER_STACK_INGESTING_URL",
    "NEXT_PUBLIC_LOGTAIL_SOURCE_TOKEN",
    "LOGTAIL_URL",
    "LOGTAIL_SOURCE_TOKEN",
    "INNGEST_APP_NAME",
    "INNGEST_SIGNING_KEY",
    "INNGEST_EVENT_KEY",
    "GITHUB_WEBHOOK_SECRET",
    "GITHUB_APP_ID",
    "GITHUB_APP_SLUG",
    "GITHUB_CLIENT_ID",
    "GITHUB_CLIENT_SECRET",
    "GITHUB_APP_PRIVATE_KEY",
    "ANTHROPIC_API_KEY",
    "OPENAI_API_KEY",
    "COHERE_API_KEY",
    "PINECONE_API_KEY",
    "MXBAI_API_KEY",
    "MXBAI_STORE_ID",
    "LIGHTFAST_API_KEY",
    "LIGHTFAST_API_URL",
    "ENCRYPTION_KEY",
    "ARCJET_KEY",
    "ARCJET_SITE_KEY",
    "RESEND_API_KEY",
    "KV_URL",
    "KV_REST_API_READ_ONLY_TOKEN",
    "KV_REST_API_TOKEN",
    "KV_REST_API_URL",
    "REDIS_URL",
    "BLOB_READ_WRITE_TOKEN",
    "BLOB_BASE_URI",
    "NEXT_PUBLIC_POSTHOG_HOST",
    "NEXT_PUBLIC_POSTHOG_KEY",
    "POSTHOG_API_KEY",
    "EXA_API_KEY",
    "BRAINTRUST_API_KEY",
    "BRAINTRUST_PROJECT_NAME",
    "BASEHUB_ADMIN_TOKEN",
    "BASEHUB_TOKEN",
    "BASEHUB_CHANGELOG_TOKEN",
    "BROWSERBASE_API_KEY",
    "BROWSERBASE_PROJECT_ID",
    "AI_GATEWAY_API_KEY",
    "NEXT_PUBLIC_API_URL",
    "HEALTH_CHECK_AUTH_TOKEN",
    "NEXT_PUBLIC_VERCEL_ENV",
    "KNOCK_API_KEY",
    "KNOCK_SIGNING_KEY",
    "OTEL_EXPORTER_OTLP_ENDPOINT",
    "OTEL_EXPORTER_OTLP_HEADERS",
    "QSTASH_URL",
    "QSTASH_TOKEN",
    "QSTASH_CURRENT_SIGNING_KEY",
    "QSTASH_NEXT_SIGNING_KEY",
    "AGENT_MAX_ITERATIONS",
    "TOOL_EXECUTION_TIMEOUT",
    "STREAM_TTL_SECONDS"
  ]
}
