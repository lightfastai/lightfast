{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json", "dist/**"]
    },
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false,
      "persistent": false
    },
    "format": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": ["^build"],
      "outputs": [".cache/.eslintcache"]
    },
    "typecheck": {
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json"]
    },
    "clean": {
      "cache": false
    },
    "migrate": {
      "cache": false
    },
    "migrate:generate": {
      "cache": false
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "dev:email": {
      "cache": false,
      "persistent": true
    },
    "ui": {
      "cache": false,
      "interactive": true
    },
    "eval": {
      "cache": false,
      "interactive": true
    }
  },
  "globalEnv": [
    "PORT",
    "POSTGRES_URL",
    "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
    "CLERK_SECRET_KEY",
    "NEXT_PUBLIC_CLERK_SIGN_IN_URL",
    "NEXT_PUBLIC_CLERK_SIGN_UP_URL",
    "NEXT_PUBLIC_CLERK_AFTER_SIGN_IN_URL",
    "NEXT_PUBLIC_CLERK_AFTER_SIGN_UP_URL",
    "ARCJET_KEY",
    "ARCJET_SITE_KEY",
    "RESEND_API_KEY",
    "NEXT_PUBLIC_API_URL",
    "NEXT_PUBLIC_LIGHTFAST_API_URL",
    "INNGEST_APP_NAME",
    "INNGEST_EVENT_KEY",
    "INNGEST_SIGNING_KEY",
    "RESEND_EARLY_ACCESS_AUDIENCE_ID",
    "REQUEST_ID_SECRET",
    "NEXT_PUBLIC_SENTRY_DSN",
    "SENTRY_DSN",
    "SENTRY_ORG",
    "SENTRY_PROJECT",
    "SENTRY_AUTH_TOKEN",
    "NEXT_PUBLIC_SENTRY_ENVIRONMENT",
    "NEXT_PUBLIC_BETTER_STACK_SOURCE_TOKEN",
    "NEXT_PUBLIC_BETTER_STACK_INGESTING_URL",
    "NEXT_PUBLIC_LOGTAIL_SOURCE_TOKEN",
    "LOGTAIL_URL",
    "LOGTAIL_SOURCE_TOKEN",
    "NEXT_PUBLIC_POSTHOG_HOST",
    "NEXT_PUBLIC_POSTHOG_KEY",
    "POSTHOG_API_KEY",
    "KV_URL",
    "KV_REST_API_TOKEN",
    "KV_REST_API_URL",
    "KV_REST_API_READ_ONLY_TOKEN",
    "REDIS_URL",
    "QSTASH_URL",
    "QSTASH_TOKEN",
    "QSTASH_CURRENT_SIGNING_KEY",
    "QSTASH_NEXT_SIGNING_KEY",
    "BRAINTRUST_API_KEY",
    "BRAINTRUST_PROJECT_NAME",
    "OTEL_EXPORTER_OTLP_ENDPOINT",
    "OTEL_EXPORTER_OTLP_HEADERS",
    "BROWSERBASE_API_KEY",
    "BROWSERBASE_PROJECT_ID",
    "ANTHROPIC_API_KEY",
    "OPENAI_API_KEY",
    "EXA_API_KEY",
    "AI_GATEWAY_API_KEY",
    "BLOB_READ_WRITE_TOKEN"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "npm_lifecycle_event",
    "ENVIRONMENT"
  ]
}
