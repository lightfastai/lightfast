{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json", "dist/**"],
      "env": [
        "NEXT_PUBLIC_*",
        "SENTRY_*",
        "VERCEL_ENV"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:inngest": {
      "cache": false,
      "persistent": true
    },
    "dev:qstash": {
      "cache": false,
      "persistent": true
    },
    "dev:ngrok": {
      "cache": false,
      "persistent": true
    },
    "dev:studio": {
      "cache": false,
      "persistent": true
    },
    "format": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": [],
      "outputs": [".cache/.eslintcache"],
      "env": []
    },
    "typecheck": {
      "dependsOn": [],
      "outputs": [".cache/tsbuildinfo.json"],
      "env": []
    },
    "clean": {
      "cache": false
    },
    "migrate": {
      "cache": false
    },
    "migrate:generate": {
      "cache": false
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "dev:email": {
      "cache": false,
      "persistent": true
    },
    "ui": {
      "cache": false,
      "interactive": true
    },
    "eval": {
      "cache": false,
      "interactive": true
    }
  },
  "globalEnv": [
    "NODE_ENV",
    "CI"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "SKIP_ENV_VALIDATION",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "VERCEL_PROJECT_PRODUCTION_URL",
    "npm_lifecycle_event",
    "ENVIRONMENT",
    "DEBUG",
    "PORT",
    "LIGHTFAST_API_KEY",
    "LIGHTFAST_API_URL"
  ]
}
