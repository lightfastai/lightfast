{
  "$schema": "https://turborepo.dev/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".cache/tsbuildinfo.json",
        "dist/**",
        ".next/**",
        "!.next/cache/**",
        "!.next/dev/**"
      ],
      "env": [
        "NEXT_PUBLIC_*",
        "SENTRY_*",
        "VERCEL_ENV"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:inngest": {
      "cache": false,
      "persistent": true
    },
    "dev:qstash": {
      "cache": false,
      "persistent": true
    },
    "dev:ngrok": {
      "cache": false,
      "persistent": true
    },
    "dev:studio": {
      "cache": false,
      "persistent": true
    },
    "format": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "transit": {
      "dependsOn": ["^transit"]
    },
    "lint": {
      "dependsOn": ["^build", "transit"],
      "outputs": [".cache/.eslintcache"],
      "env": []
    },
    "test": {
      "dependsOn": ["^build"],
      "cache": false,
      "env": []
    },
    "typecheck": {
      "dependsOn": ["^build", "transit"],
      "outputs": [".cache/tsbuildinfo.json"],
      "env": []
    },
    "clean": {
      "cache": false
    },
    "migrate": {
      "cache": false
    },
    "migrate:generate": {
      "cache": false
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "dev:email": {
      "cache": false,
      "persistent": true
    },
    "ui": {
      "cache": false,
      "interactive": true
    },
    "eval": {
      "cache": false,
      "interactive": true
    }
  },
  "globalEnv": [
    "NODE_ENV",
    "CI"
  ],
  "globalPassThroughEnv": [
    "SKIP_ENV_VALIDATION",
    "npm_lifecycle_event",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "VERCEL_PROJECT_PRODUCTION_URL"
  ],
  "boundaries": {
    "tags": {
      "vendor": {
        "dependencies": { "deny": ["packages", "data", "api", "app"] }
      },
      "internal": {
        "dependencies": { "deny": ["vendor", "packages", "data", "api", "app", "core"] }
      },
      "app": {
        "dependents": { "deny": ["vendor", "packages", "data", "api", "core", "internal"] }
      }
    }
  }
}
