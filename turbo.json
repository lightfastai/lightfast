{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [
        ".cache/tsbuildinfo.json",
        "dist/**",
        ".next/**",
        "!.next/cache/**"
      ],
      "env": [
        "NEXT_PUBLIC_*",
        "SENTRY_*",
        "VERCEL_ENV"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:inngest": {
      "cache": false,
      "persistent": true
    },
    "dev:qstash": {
      "cache": false,
      "persistent": true
    },
    "dev:ngrok": {
      "cache": false,
      "persistent": true
    },
    "dev:studio": {
      "cache": false,
      "persistent": true
    },
    "format": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": [],
      "outputs": [".cache/.eslintcache"],
      "env": []
    },
    "typecheck": {
      "dependsOn": [],
      "outputs": [".cache/tsbuildinfo.json"],
      "env": []
    },
    "clean": {
      "cache": false
    },
    "migrate": {
      "cache": false
    },
    "migrate:generate": {
      "cache": false
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "dev:email": {
      "cache": false,
      "persistent": true
    },
    "ui": {
      "cache": false,
      "interactive": true
    },
    "eval": {
      "cache": false,
      "interactive": true
    }
  },
  "globalEnv": [
    "NODE_ENV",
    "CI"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "SKIP_ENV_VALIDATION",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "VERCEL_PROJECT_PRODUCTION_URL",
    "npm_lifecycle_event",
    "ENVIRONMENT",
    "DEBUG",
    "PORT",
    "LIGHTFAST_API_KEY",
    "LIGHTFAST_API_URL",
    "GITHUB_WEBHOOK_SECRET",
    "GITHUB_APP_ID",
    "GITHUB_APP_SLUG",
    "GITHUB_CLIENT_ID",
    "GITHUB_CLIENT_SECRET",
    "GITHUB_APP_PRIVATE_KEY",
    "CLERK_SECRET_KEY",
    "CLERK_MACHINE_SECRET_KEY_INNGEST",
    "CLERK_MACHINE_SECRET_KEY_WEBHOOK",
    "CLERK_MACHINE_SECRET_KEY_TRPC",
    "CLERK_M2M_TOKEN_WEBHOOK",
    "CLERK_M2M_MACHINE_ID_WEBHOOK",
    "CLERK_M2M_TOKEN_INNGEST",
    "CLERK_M2M_MACHINE_ID_INNGEST",
    "INNGEST_APP_NAME",
    "INNGEST_SIGNING_KEY",
    "INNGEST_EVENT_KEY",
    "COHERE_API_KEY",
    "PINECONE_API_KEY",
    "BASEHUB_ADMIN_TOKEN",
    "BASEHUB_TOKEN",
    "KV_URL",
    "KV_REST_API_READ_ONLY_TOKEN",
    "KV_REST_API_TOKEN",
    "KV_REST_API_URL",
    "REDIS_URL",
    "BRAINTRUST_API_KEY",
    "SENTRY_VERCEL_LOG_DRAIN_URL",
    "SENTRY_OTLP_TRACES_URL",
    "SENTRY_PUBLIC_KEY",
    "DATABASE_HOST",
    "DATABASE_USERNAME",
    "DATABASE_PASSWORD",
    "KNOCK_API_KEY",
    "KNOCK_SIGNING_KEY",
    "ENCRYPTION_KEY"
  ]
}
