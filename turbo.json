{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "topo": {
      "dependsOn": ["^topo"]
    },
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".cache/tsbuildinfo.json", "dist/**"]
    },
    "dev": {
      "dependsOn": ["^dev"],
      "cache": false,
      "persistent": false
    },
    "format": {
      "outputs": [".cache/.prettiercache"],
      "outputLogs": "new-only"
    },
    "lint": {
      "dependsOn": ["^topo", "^build"],
      "outputs": [".cache/.eslintcache"]
    },
    "typecheck": {
      "dependsOn": ["^topo", "^build"],
      "outputs": [".cache/tsbuildinfo.json"]
    },
    "clean": {
      "cache": false
    },
    "//#clean": {
      "cache": false
    },
    "push": {
      "cache": false,
      "interactive": true
    },
    "studio": {
      "cache": false,
      "persistent": true
    },
    "migrate": {
      "cache": false
    },
    "migrate:generate:app": {
      "cache": false
    },
    "migrate:generate:tenant": {
      "cache": false
    },
    "migrate:generate:custom": {
      "cache": false
    },
    "ui-add": {
      "cache": false,
      "interactive": true
    },
    "eval": {
      "cache": false,
      "interactive": true
    }
  },
  "globalEnv": [
    "POSTGRES_URL",
    "GITHUB_ID",
    "GITHUB_SECRET",
    "OPENAI_API_KEY",
    "ANTHROPIC_API_KEY",
    "AUTH_SECRET",
    "PORT",
    "BRAINTRUST_API_KEY",
    "NEXT_PUBLIC_SENTRY_DSN",
    "SENTRY_ORG",
    "SENTRY_PROJECT",
    "SENTRY_AUTH_TOKEN",
    "NEON_API_KEY",
    "INNGEST_APP_NAME",
    "PG_VERSION",
    "DB_NAME",
    "ROLE_NAME",
    "REGION_ID",
    "ORG_ID"
  ],
  "globalPassThroughEnv": [
    "NODE_ENV",
    "CI",
    "VERCEL",
    "VERCEL_ENV",
    "VERCEL_URL",
    "npm_lifecycle_event"
  ]
}
