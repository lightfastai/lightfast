{
  "extends": "@repo/typescript-config/internal-package.json",
  "compilerOptions": {
    "types": [
      "node"
    ],
    "paths": {
      "~/env": [
        "./env.ts"
      ],
      "~/*": [
        "src/*"
      ]
    },
    "rootDir": ".",
    "baseUrl": "."
  },
  "include": [
    "env.ts",
    "src"
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}