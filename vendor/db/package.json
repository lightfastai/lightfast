{
  "name": "@vendor/db",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./lightfast/schema": {
      "types": "./dist/lightfast/schema.d.ts",
      "default": "./src/lightfast/schema/index.ts"
    },
    "./media-server/schema": {
      "types": "./dist/media-server/schema.d.ts",
      "default": "./src/media-server/schema/index.ts"
    },
    "./lightfast/types": {
      "types": "./dist/lightfast/types.d.ts",
      "default": "./src/lightfast/types.ts"
    },
    "./media-server/types": {
      "types": "./dist/media-server/types.d.ts",
      "default": "./src/media-server/types.ts"
    },
    "./env": "./env.ts",
    "./create-drizzle-config": "./src/utils/create-drizzle-config.ts"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "lightfast:migrate": "pnpm with-env drizzle-kit migrate --config=./src/lightfast/drizzle.config.ts",
    "lightfast:migrate:generate": "pnpm with-env drizzle-kit generate --config=./src/lightfast/drizzle.config.ts",
    "lightfast:studio": "pnpm with-env drizzle-kit studio --config=./src/lightfast/drizzle.config.ts",
    "media-server:migrate": "pnpm with-env-media-server drizzle-kit migrate --config=./src/media-server/drizzle.config.ts",
    "media-server:migrate:generate": "pnpm with-env-media-server drizzle-kit generate --config=./src/media-server/drizzle.config.ts",
    "media-server:studio": "pnpm with-env-media-server drizzle-kit studio --config=./src/media-server/drizzle.config.ts",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --",
    "with-env-lightfast": "dotenv -e ../../.env.lightfast --",
    "with-env-media-server": "dotenv -e ../../apps/media-server/.internal/.env.development.local --"
  },
  "dependencies": {
    "@neondatabase/serverless": "^1.0.0",
    "@repo/ai": "workspace:*",
    "@repo/lib": "workspace:*",
    "@repo/webgl": "workspace:^",
    "drizzle-orm": "^0.35.3",
    "drizzle-zod": "^0.5.1",
    "postgres": "^3.4.5",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "dotenv-cli": "^7.4.2",
    "drizzle-kit": "^0.26.2",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@repo/prettier-config"
}
