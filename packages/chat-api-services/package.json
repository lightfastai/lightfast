{
  "name": "@repo/chat-api-services",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    "./artifacts": {
      "types": "./dist/artifacts.d.ts",
      "default": "./src/artifacts.ts"
    },
    "./messages": {
      "types": "./dist/messages.d.ts",
      "default": "./src/messages.ts"
    },
    "./sessions": {
      "types": "./dist/sessions.d.ts",
      "default": "./src/sessions.ts"
    },
    "./streams": {
      "types": "./dist/streams.d.ts",
      "default": "./src/streams.ts"
    },
    "./usage": {
      "types": "./dist/usage.d.ts",
      "default": "./src/usage.ts"
    },
    "./quota-reservation": {
      "types": "./dist/quota-reservation.d.ts",
      "default": "./src/quota-reservation.ts"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "default": "./src/errors.ts"
    },
    "./base-service": {
      "types": "./dist/base-service.d.ts",
      "default": "./src/base-service.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@api/chat": "workspace:*",
    "@repo/chat-ai-types": "workspace:*",
    "@repo/chat-trpc": "workspace:*",
    "@trpc/client": "catalog:",
    "@trpc/server": "catalog:",
    "date-fns-tz": "^3.2.0"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@repo/prettier-config"
}
