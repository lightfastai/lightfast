{
	"name": "@lightfast/ai",
	"version": "1.0.0",
	"description": "AI agents, tools, and workflows for Lightfast",
	"exports": {
		"./agent": {
			"types": "./src/core/primitives/agent.ts",
			"default": "./src/core/primitives/agent.ts"
		},
		"./tool": {
			"types": "./src/core/primitives/tool.ts",
			"default": "./src/core/primitives/tool.ts"
		},
		"./agent/handlers": {
			"types": "./src/core/server/adapters/fetch.ts",
			"default": "./src/core/server/adapters/fetch.ts"
		},
		"./agent/server/adapters/fetch": {
			"types": "./src/core/server/adapters/fetch.ts",
			"default": "./src/core/server/adapters/fetch.ts"
		},
		"./agent/server/adapters/types": {
			"types": "./src/core/server/adapters/types.ts",
			"default": "./src/core/server/adapters/types.ts"
		},
		"./agent/memory": {
			"types": "./src/core/memory/index.ts",
			"default": "./src/core/memory/index.ts"
		},
		"./agent/memory/adapters/redis": {
			"types": "./src/core/memory/adapters/redis.ts",
			"default": "./src/core/memory/adapters/redis.ts"
		},
		"./agent/memory/redis": {
			"types": "./src/core/memory/adapters/redis.ts",
			"default": "./src/core/memory/adapters/redis.ts"
		},
		"./agent/redis-memory": {
			"types": "./src/core/memory/adapters/redis.ts",
			"default": "./src/core/memory/adapters/redis.ts"
		},
		"./agent/primitives/cache": {
			"types": "./src/core/primitives/cache/index.ts",
			"default": "./src/core/primitives/cache/index.ts"
		},
		"./providers": {
			"types": "./src/utils/providers.ts",
			"default": "./src/utils/providers.ts"
		},
		"./v2/server": {
			"types": "./src/core/v2/server/index.ts",
			"default": "./src/core/v2/server/index.ts"
		},
		"./v2/react": {
			"types": "./src/core/v2/react/index.ts",
			"default": "./src/core/v2/react/index.ts"
		}
	},
	"scripts": {
		"clean": "git clean -xdf .cache .turbo dist node_modules",
		"build": "tsc",
		"dev": "tsc",
		"typecheck": "tsc --noEmit --emitDeclarationOnly false",
		"lint": "biome check --write ."
	},
	"dependencies": {
		"@ai-sdk/anthropic": "catalog:",
		"@ai-sdk/gateway": "catalog:",
		"@ai-sdk/provider": "catalog:",
		"@t3-oss/env-core": "catalog:",
		"@upstash/redis": "catalog:",
		"ai": "catalog:",
		"braintrust": "catalog:",
		"nanoid": "^5.1.5",
		"playwright": "catalog:",
		"playwright-core": "catalog:",
		"resumable-stream": "catalog:",
		"zod": "catalog:"
	},
	"devDependencies": {
		"@types/node": "^24.0.15",
		"@types/react": "^19.1.8",
		"typescript": "catalog:"
	}
}
