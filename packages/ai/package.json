{
	"name": "@lightfast/ai",
	"version": "1.0.0",
	"description": "AI agents, tools, and workflows for Lightfast",
	"exports": {
		"./server": {
			"types": "./src/mastra/index.ts",
			"default": "./src/mastra/index.ts"
		},
		"./agents": {
			"types": "./src/mastra/agents/index.ts",
			"default": "./src/mastra/agents/index.ts"
		},
		"./agents/experimental": {
			"types": "./src/mastra/agents/experimental/index.ts",
			"default": "./src/mastra/agents/experimental/index.ts"
		},
		"./agents/experimental/a011": {
			"types": "./src/mastra/agents/experimental/a011.ts",
			"default": "./src/mastra/agents/experimental/a011.ts"
		},
		"./tools": {
			"types": "./src/mastra/tools/index.ts",
			"default": "./src/mastra/tools/index.ts"
		},
		"./env": {
			"types": "./src/env.ts",
			"default": "./src/env.ts"
		}
	},
	"scripts": {
		"clean": "git clean -xdf .cache .turbo dist node_modules",
		"build": "tsc",
		"dev": "tsc",
		"typecheck": "tsc --noEmit --emitDeclarationOnly false",
		"lint": "biome check --write .",
		"dev:mastra": "mastra dev --dir src/mastra",
		"build:mastra": "mastra build --dir src/mastra"
	},
	"dependencies": {
		"@ai-sdk/anthropic": "catalog:",
		"@ai-sdk/gateway": "catalog:",
		"@ai-sdk/provider": "catalog:",
		"@browserbasehq/stagehand": "catalog:",
		"@mastra/core": "catalog:",
		"@mastra/evals": "catalog:",
		"@mastra/memory": "catalog:",
		"@mastra/upstash": "catalog:",
		"@mastra/voice-elevenlabs": "catalog:",
		"@mastra/voice-openai": "catalog:",
		"@mastra/voice-openai-realtime": "catalog:",
		"@t3-oss/env-core": "catalog:",
		"@upstash/redis": "catalog:",
		"@vercel/blob": "catalog:",
		"@vercel/sandbox": "catalog:",
		"ai": "catalog:",
		"braintrust": "catalog:",
		"exa-js": "catalog:",
		"playwright": "catalog:",
		"playwright-core": "catalog:",
		"zod": "catalog:"
	},
	"devDependencies": {
		"@lightfast/types": "workspace:*",
		"mastra": "catalog:",
		"typescript": "catalog:"
	}
}
