{
  "name": "@repo/ai",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./fal-env": "./src/env/fal-env.ts",
    "./openai-env": "./src/env/openai-env.ts",
    "./anthropic-env": "./src/env/anthropic-env.ts",
    "./browserbase-env": "./src/env/browserbase-env.ts",
    "./openrouter-env": "./src/env/openrouter-env.ts",
    "./google-env": "./src/env/google-env.ts",
    "./schema": {
      "types": "./dist/src/schema/index.d.ts",
      "default": "./src/schema/index.ts"
    },
    "./fal": "./src/fal/client.ts",
    "./fal/server": "./src/fal/nextjs-server-proxy.ts",
    "./fal/hono-server": "./src/fal/hono-server-proxy.ts",
    "./ai": "./src/ai/index.ts"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "eval": "pnpm with-env braintrust eval ./src/index.eval.ts",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false",
    "with-env": "dotenv -e ../../.env --"
  },
  "dependencies": {
    "@ai-sdk/anthropic": "2.0.4",
    "@ai-sdk/google": "^1.2.17",
    "@ai-sdk/google-vertex": "^2.2.21",
    "@ai-sdk/openai": "^0.0.70",
    "@ai-sdk/react": "^2.0.14",
    "@fal-ai/client": "^1.2.0",
    "@fal-ai/server-proxy": "^1.1.1",
    "@openrouter/ai-sdk-provider": "^0.4.5",
    "@t3-oss/env-nextjs": "^0.12.0",
    "ai": "5.0.15",
    "autoevals": "^0.0.103",
    "braintrust": "^0.2.1",
    "exa-js": "^1.6.13",
    "resumable-stream": "^2.0.0",
    "voyage-ai-provider": "^1.0.0",
    "zod": "catalog:"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:"
  },
  "prettier": "@repo/prettier-config"
}
