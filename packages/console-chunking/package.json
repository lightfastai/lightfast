{
  "name": "@repo/console-chunking",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./chunk": "./src/chunk.ts",
    "./mdx": "./src/mdx.ts",
    "./hash": "./src/hash.ts"
  },
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsup --watch",
    "format": "prettier --check . --ignore-path ../../.gitignore",
    "lint": "eslint",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@repo/console-types": "workspace:*",
    "gray-matter": "^4.0.3",
    "js-tiktoken": "^1.0.15"
  },
  "devDependencies": {
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "tsup": "^8.5.0",
    "typescript": "catalog:"
  },
  "prettier": "@repo/prettier-config"
}
