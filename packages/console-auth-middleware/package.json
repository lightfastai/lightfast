{
  "name": "@repo/console-auth-middleware",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./src/index.ts"
    },
    "./workspace": {
      "types": "./dist/workspace.d.ts",
      "default": "./src/workspace.ts"
    },
    "./resources": {
      "types": "./dist/resources.d.ts",
      "default": "./src/resources.ts"
    },
    "./tenant": {
      "types": "./dist/tenant.d.ts",
      "default": "./src/tenant.ts"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "default": "./src/types.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
