{
  "name": "@repo/console-reserved-names",
  "version": "0.1.0",
  "private": true,
  "description": "Reserved workspace names for Lightfast Console to prevent URL routing conflicts",
  "license": "FSL-1.1-Apache-2.0",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./workspace": {
      "types": "./dist/workspace.d.ts",
      "default": "./dist/workspace.js"
    },
    "./organization": {
      "types": "./dist/organization.d.ts",
      "default": "./dist/organization.js"
    }
  },
  "files": [
    "dist",
    "data"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "lightfast",
    "reserved",
    "workspace",
    "organization",
    "validation"
  ],
  "devDependencies": {
    "@repo/typescript-config": "workspace:*",
    "typescript": "catalog:"
  }
}
