{
	"name": "@lightfast/www-search",
	"version": "0.1.0",
	"private": true,
	"license": "Apache-2.0",
	"type": "module",
	"scripts": {
		"build": "pnpm build:prod",
		"build:dev": "pnpm with-env:dev next build --turbopack",
		"build:prod": "pnpm with-env:prod next build --turbopack",
		"clean": "git clean -xdf .cache .next .turbo .vercel node_modules",
		"dev": "pnpm with-env:dev next dev --port $(microfrontends port) --turbo",
		"format": "prettier --check . --ignore-path ../../.gitignore",
		"lint": "eslint",
		"start": "pnpm with-env:prod next start -p ${NEXT_PUBLIC_SEARCH_PORT:-4105}",
		"typecheck": "tsc --noEmit",
		"with-env:dev": "dual run --",
		"with-env:prod": "dotenv -e ./.vercel/.env.development.local --"
	},
	"dependencies": {
		"@repo/app-urls": "workspace:*",
		"@repo/site-config": "workspace:*",
		"@repo/ui": "workspace:*",
		"@repo/url-utils": "workspace:*",
		"@sentry/nextjs": "^10.20.0",
		"@t3-oss/env-nextjs": "catalog:",
		"@vendor/clerk": "workspace:*",
		"@vendor/next": "workspace:*",
		"@vendor/security": "workspace:*",
		"@vendor/seo": "workspace:*",
		"@vendor/upstash": "workspace:*",
		"@vercel/microfrontends": "^2.0.1",
		"@vercel/related-projects": "catalog:",
		"framer-motion": "^11.3.19",
		"geist": "catalog:",
		"lucide-react": "catalog:",
		"neverthrow": "catalog:",
		"next": "catalog:",
		"react": "19.2.0",
		"react-confetti": "^6.4.0",
		"react-dom": "19.2.0",
		"zod": "catalog:"
	},
	"devDependencies": {
		"@repo/eslint-config": "workspace:*",
		"@repo/prettier-config": "workspace:*",
		"@repo/typescript-config": "workspace:*",
		"@tailwindcss/postcss": "catalog:tailwind4",
		"@tailwindcss/typography": "catalog:tailwind4",
		"@types/node": "catalog:",
		"@types/react": "19.2.2",
		"@types/react-dom": "19.2.2",
		"autoprefixer": "^10.4.21",
		"dotenv-cli": "^8.0.0",
		"eslint": "catalog:",
		"import-in-the-middle": "catalog:",
		"jiti": "^1.21.6",
		"postcss": "catalog:tailwind4",
		"require-in-the-middle": "catalog:",
		"tailwindcss": "catalog:tailwind4",
		"typescript": "catalog:",
		"typescript-eslint": "catalog:"
	},
	"prettier": "@repo/prettier-config"
}
