{
  "name": "@lightfast/gateway",
  "license": "FSL-1.1-Apache-2.0",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsup && pnpm with-env node dist/src/env.js",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "pnpm with-env srvx --import tsx -p 4108 src/index.ts",
    "lint": "eslint",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ./.vercel/.env.development.local --"
  },
  "dependencies": {
    "@db/console": "workspace:*",
    "@neondatabase/serverless": "^1.0.2",
    "@repo/console-validation": "workspace:*",
    "@repo/gateway-types": "workspace:*",
    "@repo/lib": "workspace:*",
    "@t3-oss/env-core": "catalog:",
    "@vendor/db": "workspace:*",
    "@vendor/qstash": "workspace:*",
    "@vendor/related-projects": "workspace:*",
    "@vendor/upstash": "workspace:*",
    "@vendor/upstash-workflow": "workspace:*",
    "drizzle-orm": "catalog:",
    "hono": "catalog:",
    "zod": "catalog:zod3"
  },
  "devDependencies": {
    "@repo/console-test-data": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "dotenv-cli": "^8.0.0",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "srvx": "^0.11.8",
    "tsup": "^8.5.0",
    "tsx": "^4.19.2",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "prettier": "@repo/prettier-config"
}
