{
  "$schema": "https://turborepo.org/schema.json",
  "extends": ["//"],
  "tags": ["app"],
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "env": [
        "NEXT_PUBLIC_VERCEL_ENV",
        "NEXT_PUBLIC_VERCEL_URL",
        "NEXT_PUBLIC_SENTRY_DSN",
        "NEXT_PUBLIC_BETTER_STACK_SOURCE_TOKEN",
        "NEXT_PUBLIC_BETTER_STACK_INGESTING_URL",
        "NEXT_PUBLIC_LOGTAIL_SOURCE_TOKEN",
        "NEXT_PUBLIC_CLERK_PUBLISHABLE_KEY",
        "NEXT_PUBLIC_POSTHOG_KEY",
        "NEXT_PUBLIC_POSTHOG_HOST"
      ],
      "passThroughEnv": [
        "SENTRY_AUTH_TOKEN",
        "SENTRY_ORG",
        "SENTRY_PROJECT",
        "CLERK_SECRET_KEY",
        "LOGTAIL_SOURCE_TOKEN",
        "LOGTAIL_URL",
        "RESEND_API_KEY",
        "INNGEST_APP_NAME",
        "INNGEST_EVENT_KEY",
        "INNGEST_SIGNING_KEY",
        "ARCJET_ENV",
        "ARCJET_KEY",
        "KV_REST_API_URL",
        "KV_REST_API_TOKEN",
        "BASEHUB_TOKEN",
        "BASEHUB_ADMIN_TOKEN",
        "BASEHUB_CHANGELOG_TOKEN",
        "HEALTH_CHECK_AUTH_TOKEN",
        "PORT"
      ],
      "outputs": [".next/**", "!.next/cache/**", "!.next/dev/**", "next-env.d.ts"]
    },
    "dev": {
      "persistent": true
    },
    "transit": {}
  }
}
