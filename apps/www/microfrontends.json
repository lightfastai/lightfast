{
  "$schema": "https://openapi.vercel.sh/microfrontends.json",
  "applications": {
    "lightfast-www": {
      "packageName": "@lightfast/www",
      "development": {
        "local": 4101,
        "fallback": "lightfast.ai"
      }
    },
    "lightfast-auth": {
      "packageName": "@lightfast/auth",
      "development": {
        "local": 4104
      },
      "routing": [
        {
          "group": "auth",
          "paths": ["/sign-in", "/sign-in/:path*", "/sign-up"]
        }
      ]
    },
    "lightfast-console": {
      "packageName": "@lightfast/console",
      "development": {
        "local": 4107
      },
      "routing": [
        {
          "group": "console",
          "paths": [
            "/new",
            "/new/:path",
            "/org",
            "/org/:path*",
            "/onboarding",
            "/onboarding/:path*",
            "/api/github",
            "/api/github/:path*",
            "/api/organizations",
            "/api/organizations/:path*",
            "/api/trpc",
            "/api/trpc/:path*",
            "/api/inngest"
          ]
        }
      ]
    }
  }
}
