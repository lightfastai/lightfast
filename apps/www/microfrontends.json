{
  "$schema": "https://openapi.vercel.sh/microfrontends.json",
  "applications": {
    "lightfast-www": {
      "packageName": "@lightfast/www",
      "development": {
        "local": 4101,
        "fallback": "lightfast.ai"
      }
    },
    "lightfast-auth": {
      "packageName": "@lightfast/auth",
      "development": {
        "local": 4104
      },
      "routing": [
        {
          "group": "auth",
          "paths": [
            "/sign-in",
            "/sign-in/:path*",
            "/sign-up"
          ]
        }
      ]
    },
    "lightfast-console": {
      "packageName": "@lightfast/console",
      "development": {
        "local": 4107
      },
      "routing": [
        {
          "group": "console",
          "paths": [
            "/app",
            "/org",
            "/org/:path*",
            "/onboarding",
            "/onboarding/:path*"
          ]
        }
      ]
    }
  }
}
