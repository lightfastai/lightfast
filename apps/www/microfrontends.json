{
  "$schema": "https://openapi.vercel.sh/microfrontends.json",
  "applications": {
    "lightfast-www": {
      "packageName": "@lightfast/www",
      "development": {
        "local": 4101,
        "fallback": "lightfast.ai"
      }
    },
    "lightfast-auth": {
      "packageName": "@lightfast/auth",
      "assetPrefix": "auth",
      "development": {
        "local": 4104
      },
      "routing": [
        {
          "group": "auth",
          "paths": ["/auth", "/auth/:path*"]
        }
      ]
    },
    "lightfast-console": {
      "packageName": "@lightfast/console",
      "assetPrefix": "console",
      "development": {
        "local": 4107
      },
      "routing": [
        {
          "group": "console",
          "paths": ["/console", "/console/:path*"]
        }
      ]
    }
  }
}
