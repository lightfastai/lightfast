{
  "name": "@lightfast/relay",
  "license": "FSL-1.1-Apache-2.0",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/index.ts"
    }
  },
  "scripts": {
    "build": "tsup && pnpm with-env node dist/src/env.js",
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "dev": "NODE_ENV=development pnpm with-env srvx --prod --import tsx -p 4108 src/index.ts",
    "lint": "eslint",
    "test": "vitest run",
    "typecheck": "tsc --noEmit",
    "with-env": "dotenv -e ./.vercel/.env.development.local --"
  },
  "dependencies": {
    "@db/console": "workspace:*",
    "@neondatabase/serverless": "catalog:",
    "@repo/console-validation": "workspace:*",
    "@repo/gateway-types": "workspace:*",
    "@repo/lib": "workspace:*",
    "@sentry/core": "catalog:",
    "@t3-oss/env-core": "catalog:",
    "@vendor/db": "workspace:*",
    "@vendor/qstash": "workspace:*",
    "@vendor/related-projects": "workspace:*",
    "@vendor/upstash": "workspace:*",
    "@vendor/upstash-workflow": "workspace:*",
    "drizzle-orm": "catalog:",
    "hono": "catalog:",
    "zod": "catalog:zod3"
  },
  "devDependencies": {
    "@repo/console-test-data": "workspace:*",
    "@repo/eslint-config": "workspace:*",
    "@repo/prettier-config": "workspace:*",
    "@repo/typescript-config": "workspace:*",
    "@types/node": "catalog:",
    "dotenv-cli": "catalog:",
    "eslint": "catalog:",
    "prettier": "catalog:",
    "srvx": "^0.11.8",
    "tsup": "^8.5.0",
    "tsx": "^4.19.2",
    "typescript": "catalog:",
    "vitest": "catalog:"
  },
  "prettier": "@repo/prettier-config"
}
